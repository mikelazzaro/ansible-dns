command[check_disk]=/usr/lib/nagios/plugins/check_disk -w 20% -c 10% -p / -p /boot
command[check_disk_inodes]=/usr/lib/nagios/plugins/check_disk -W 50% -K 25% -p / -p /boot
command[check_ssh]=/usr/lib/nagios/plugins/check_ssh -H localhost
command[check_dns]=/usr/lib/nagios/plugins/check_dns -H parksys01.sys.otg.localdomain -s localhost
command[check_users]=/usr/lib/nagios/plugins/check_users -w 5 -c 10
command[check_load]=/usr/lib/nagios/plugins/check_load -w 15,10,5 -c 30,25,20
command[check_memory]=/usr/lib/nagios/plugins/check_mem -w 80 -c 90
command[check_procs]=/usr/lib/nagios/plugins/check_procs -w 400 -c 450
command[check_ntp]=/usr/lib/nagios/plugins/check_ntp_time -H 10.100.10.30 -w 1 -c 5

command[check_logstash_beats]=/usr/lib/nagios/plugins/check_tcp -H {{ logstash_host }} -p 5044
command[check_logstash_beats_secondary]=/usr/lib/nagios/plugins/check_tcp -H {{ logstash_host_secondary }} -p 5044

command[check_process_telegraf]=/usr/lib/nagios/plugins/check_procs -c1:1 -w1:1 -C telegraf
command[check_process_filebeat]=/usr/lib/nagios/plugins/check_procs -c1:1 -w1:1 -C filebeat
