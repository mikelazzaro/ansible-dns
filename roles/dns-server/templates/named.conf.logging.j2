{{ warning_message_slash }}

logging {
{% for channel in log_channels %}
    channel {{ channel }}_file {
        file "{{ named_log_folder }}/{{ channel }}.log" versions 3 size 5m;
        severity dynamic;
        print-time yes;
        print-severity yes;
    };
{% endfor %}

{% for channel in log_channels %}
    category {{ channel }} { {{ channel }}_file; };
{% endfor %}
};
